{{- if not .Extra.Checks }}[]
{{ else }}{{- range .Extra.Checks }}
- criticality: error
  check:
    function: {{ .Function }}
{{- if .Args }}
    arguments:
{{- range .Args }}
{{- if .List }}
      {{ .Key }}:
{{- range .List }}
        - {{ . }}
{{- end }}
{{- else }}
      {{ .Key }}: {{ .Value }}
{{- end }}
{{- end }}
{{- end }}
{{ end }}{{ end }}